Attribute VB_Name = "Items"


Sub add(ByVal value As Long)
Dim nn As Long
For nn = 1 To UBound(LootBox)
If LootBox(nn).BoxID = 0 Then
LootBox(nn).BoxID = value
LootBox(nn).BoxOpened = False
Exit For
End If
Next
End Sub

Sub GetCooldown(searchID As Long)
Dim av As Long
For av = 1 To UBound(Skills)
If Skills(av).Class = Class Then
If Skills(av).id = searchID Then
Form1.Text3.text = Skills(av).Cooldown
Exit Sub
End If
End If
Next
End Sub

Sub Remove(value As Long)
Dim hm As Long
For hm = 1 To UBound(LootBox)
If LootBox(hm).BoxID = value Then
LootBox(hm).BoxID = 0
End If
Next
End Sub

